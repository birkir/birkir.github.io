{
  "name": "birkir-github-io",
  "description": "My personal blog",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "birkir",
    "blog",
    "gatsby",
    "typescript",
    "prime"
  ],
  "author": "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/birkir/birkir.github.io"
  },
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "start": "gatsby serve --port ${PORT:-3000}",
    "clean": "rimraf .cache public",
    "prettier": "prettier --write '**/*.{json,md,js,jsx,ts,tsx}'",
    "lint": "npm run tslint && npm run stylelint",
    "tslint": "tslint --fix 'src/**/*.{ts,tsx}' -p .",
    "stylelint": "stylelint 'src/**/*.scss' --syntax scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "gatsby": "2.0.91",
    "gatsby-image": "2.0.26",
    "gatsby-plugin-config": "1.0.7",
    "gatsby-plugin-layout": "1.0.11",
    "gatsby-plugin-manifest": "2.0.13",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-react-svg": "2.0.0",
    "gatsby-plugin-sharp": "2.0.17",
    "gatsby-plugin-typescript": "2.0.3",
    "gatsby-plugin-ueno": "2.0.1",
    "gatsby-source-graphql-universal": "^2.0.4",
    "gatsby-transformer-sharp": "2.1.10",
    "gsap": "2.0.2",
    "gsap-tools": "1.0.9",
    "lodash": "4.17.11",
    "prismjs": "1.15.0",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "react-helmet": "5.2.0",
    "react-markdown": "4.0.6",
    "react-prism": "4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@types/gsap": "1.20.1",
    "@types/lodash": "4.14.119",
    "@types/node": "10.12.18",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-helmet": "5.0.8",
    "@ueno/stylelint-config": "1.1.2",
    "@ueno/tslint-config": "1.0.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "stylelint": "9.9.0",
    "tslint": "5.12.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.1"
  }
}
